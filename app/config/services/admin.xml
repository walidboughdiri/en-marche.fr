        # Interactives
        ######################################################
         -->

        <service id="app.admin.ton_macron_choice" class="AppBundle\Admin\TonMacronChoiceAdmin">
            <tag name="sonata.admin" manager-type="orm" label="Ton Macron » Choix" group="Interactifs" />

            <argument />
