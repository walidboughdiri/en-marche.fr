            <argument />
            <argument>AppBundle\Entity\BoardMember\Role</argument>
            <argument />
        </service>

        <!--
        ######################################################
        # Legislatives
        ######################################################
         -->

        <service id="app.admin.legislative_candidate" class="AppBundle\Admin\LegislativeCandidateAdmin">
